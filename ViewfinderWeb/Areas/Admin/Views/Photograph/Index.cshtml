

<div class="row mt-4">
    <div class="col-6">
        <h3 class="text-primary">Photograph List</h3>
    </div>
    <div class="col-6 text-end">
        <a asp-action="Upsert" asp-controller="Photograph" class="btn btn-primary"><i class="bi bi-plus-circle"></i>&nbsp;Upload Photograph</a>
    </div>
</div>
<br />
<table id="tblData" class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Title</th>
            <th>Author</th>
            <th>Description</th>
            <th>Price</th>
            <th>Category</th>
            <th>Composition</th>
            <th>Action</th>
        </tr>
    </thead>
</table>

@section Scripts{
    <script src="~/js/photograph.js"></script>
}
